/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=b,out=invb);
    Not(in=a,out=inva);
    And(a=a,b=invb,out=c);
    And(a=inva,b=b,out=d);
    Or(a=c,b=d,out=out);

}